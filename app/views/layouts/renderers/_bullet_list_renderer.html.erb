<% case bullet_list[:type] %>
<% when :unordered %>
  <%= render 'layouts/components/bullet_lists/unordered_bullet_list', items: bullet_list[:items] %>
<% when :ordered %>
  <%= render 'layouts/components/bullet_lists/ordered_bullet_list', items: bullet_list[:items], large_numbers: bullet_list[:large_numbers] %>
<% end %>
